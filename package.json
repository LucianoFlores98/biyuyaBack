  "description": "Esqueleto de node.js que implementa arquitectura hexagonal con principios SOLID con una base de datos relacional",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll --verbose",
    "dev": "nodemon",
    "migrations:run": "npx sequelize-cli db:migrate",
